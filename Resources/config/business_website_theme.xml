<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="themes.businesswebsitetheme_home.stylesheets" type="collection">
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/reset.css</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/layout.css</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/style.css</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/al_fix_style.css</parameter>
        </parameter>
        <parameter key="themes.businesswebsitetheme_home.stylesheet_cms" type="collection">
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/cms_fix.css</parameter>
        </parameter>
        <parameter key="themes.businesswebsitetheme_home.javascripts" type="collection">
            <parameter>@AlphaLemonThemeEngineBundle/Resources/public/js/vendor/jquery/*</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/cufon-yui.js</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/al-cufon-replace.js</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/Swis721_Cn_BT_400.font.js</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/Swis721_Cn_BT_700.font.js</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/jquery.easing.1.3.js</parameter>            
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/jcarousellite.js</parameter>
        </parameter>
        <parameter key="themes.businesswebsitetheme_sixboxes.stylesheets" type="collection">
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/reset.css</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/layout.css</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/style.css</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/al_fix_style.css</parameter>
        </parameter>
        <parameter key="themes.businesswebsitetheme_rightcolumn.stylesheets" type="collection">
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/reset.css</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/layout.css</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/style.css</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/al_fix_style.css</parameter>
        </parameter>        
        <parameter key="themes.businesswebsitetheme_rightcolumn.javascripts" type="collection">
            <parameter>@AlphaLemonThemeEngineBundle/Resources/public/js/vendor/jquery/*</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/cufon-yui.js</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/cufon-replace.js</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/Swis721_Cn_BT_400.font.js</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/Swis721_Cn_BT_700.font.js</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/tabs.js</parameter>
        </parameter>
        <parameter key="themes.businesswebsitetheme_fullpage.stylesheets" type="collection">
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/reset.css</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/layout.css</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/style.css</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/css/al_fix_style.css</parameter>
        </parameter>
        <parameter key="themes.businesswebsitetheme_fullpage.javascripts" type="collection">
            <parameter>@AlphaLemonThemeEngineBundle/Resources/public/js/vendor/jquery/*</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/cufon-yui.js</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/cufon-replace.js</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/Swis721_Cn_BT_400.font.js</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/Swis721_Cn_BT_700.font.js</parameter>
            <parameter>@BusinessWebsiteThemeBundle/Resources/public/js/tabs.js</parameter>
        </parameter>
        
        <parameter key="after_block_edit.class">AlphaLemon\Theme\BusinessWebsiteThemeBundle\Core\Listener\AfterEditBlockListener</parameter>
    </parameters>
    
    <services>
        <service id="after_block_edit.class" class="%after_block_edit.class%">
            <tag name="alcms.event_listener" event="actions.block_edited" method="onBlockEdited" priority="-255" />
            <argument type="service" id="service_container" />
        </service>
    </services>
</container>